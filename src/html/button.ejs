        <h2 class="docs-component__title">button type</h2>

            <div class="docs-component__preview">
                <button type="button" class="btn"><span class="btn-text">default</span></button>
                <button type="button" class="btn btn-primary"><span class="btn-text">primary</span></button>
                <button type="button" class="btn btn-secondary"><span class="btn-text">secondary</span></button>
