                <input type="submit" class="btn" value="submit">
                <input type="reset" class="btn" value="reset">
            </script></div>




        <h2 class="docs-component__title">button disabled</h2>

            <div class="docs-component__preview">
                <a href="javascript:void(0);" class="btn is-disabled"><span class="btn-text">anchor</span></a>
                <button type="button" class="btn" disabled><span class="btn-text">button</span></button>
                <input type="button" class="btn" value="input" disabled>
