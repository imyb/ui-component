            </script></div>




        <h2 class="docs-component__title">button color</h2>

            <div class="docs-component__preview">
                <button type="button" class="btn btn-gray"><span class="btn-text">gray</span></button>
                <button type="button" class="btn btn-dark"><span class="btn-text">dark</span></button>
                <button type="button" class="btn btn-green"><span class="btn-text">green</span></button>
