            <div class="docs-component__preview">
                <button type="button" class="btn"><i class="iconset icon-checked" aria-hidden="true"></i><span class="btn-text">button</span></button>
                <button type="button" class="btn"><span class="btn-text">button</span><i class="iconset icon-close" aria-hidden="true"></i></button>
